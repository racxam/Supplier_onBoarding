<mvc:View xmlns:odata="sap.ui.comp.odata"
    controllerName="com.sumo.supplieronboarding.controller.RegistrationForm"
     xmlns:sample="com.sumo.supplieronboarding.controller.blocks"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:uxap="sap.uxap"
    xmlns="sap.m"
    xmlns:upload="sap.m.upload"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
>
    <Page
        id="page6"
        title="{i18n>SRegisForm}"
    >
        <content>
    <uxap:ObjectPageLayout
        id="ObjectPageLayout2"
        upperCaseAnchorBar="false"
    >
        <uxap:sections>
            <uxap:ObjectPageSection
                titleUppercase="false"
                id="SRQFsection"
                title="Section 1"
                class="custom-section-spacing"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="section_SS1"
                        title="Supplier Registration Questionnaire Form"
                        titleUppercase="false"
                    >
                        <!-- <uxap:blocks>
                            <sample:SRQF id="bbt4" />
                        </uxap:blocks> -->
         <VBox class="sapUiSmallMargin SRFForm">
            <f:SimpleForm
                id="SimpleFormSRQF"
                editable="true"
                layout="ColumnLayout"
                labelSpanXL="6"
                labelSpanL="3"
                labelSpanM="6"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="0"
                emptySpanL="4"
                emptySpanM="0"
                emptySpanS="0"
                columnsXL="1"
                columnsL="1"
                columnsM="1"
                singleContainerFullSize="false"
                ariaLabelledBy="Title1"
            >
                <f:toolbar>
                    <Toolbar id="TB6">
                        <Title
                            id="Title5"
                            text="Supplier Registration Questionnaire Form"
                        />
                        <ToolbarSpacer />
                    </Toolbar>
                </f:toolbar>
                <f:content>
                    <Label
                        text="Supplier Legal Full Name"
                        required='true'
                        id="SupplierNameID"
                    />
                    <Input value="{/SupplierNameValue}" />

                    <Label
                        text="Supplier Organization Type"
                        required='true'
                    />
                    <ComboBox
                        id="CountryID"
                        selectedKey="Country"
                    >
                        <items>
                            <core:Item
                                text="Individual"
                                key="Individual"
                            />
                            <core:Item
                                text="Company"
                                key="Company"
                            />
                            <core:Item
                                text="Partnership Firms"
                                key="Partnership Firms"
                            />
                            <core:Item
                                text="HUF"
                                key="HUF"
                            />
                            <core:Item
                                text="Trust"
                                key="Trust"
                            />
                            <core:Item
                                text="LLP"
                                key="LLP"
                            />
                            <core:Item
                                text="Local Authority"
                                key="Local Authority"
                            />
                            <core:Item
                                text="Government"
                                key="Government"
                            />
                            <core:Item
                                text="Body of individuals"
                                key="Body of individuals"
                            />
                        </items>
                    </ComboBox>
                    <Label text="Main Telephone Number" />
                    <Input placeholder="Street" />
                    <Label
                        id="MahindracodeofbusinessID"
                        text="Do you accept the Mahindra code of business conduct?"
                    />
                    <RadioButtonGroup
                        id="RadioGroup_ID"
                        columns="2"
                        selectedIndex="0"
                    >
                        <buttons>
                            <RadioButton
                                id="RB1ID"
                                text="Yes"
                            />
                            <RadioButton
                                id="RB2ID"
                                text="No"
                            />
                        </buttons>
                    </RadioButtonGroup>
                    <Label
                    id="AcceptedSupplierCodeID"
                        displayOnly="true"
                        wrapping="true"
                        text="Please upload the Signed Copy of Accepted Supplier Code of Conduct Declaration."
                    />

                    <Label
                    id="VendorEvaluation_ID"
                        text="Vendor Evaluation"
                        required="true"
                    />
                    <unified:FileUploader
                        id="VendorEvafileUploader"
                        fileType="jpg,png,pdf,docx"
                        change="onFileUpload"
                        width="100%"
                    />
                    <Button
                        text="{path: '/documentfiles', formatter: '.formatAttachmentButtonText'}"
                        press="onOpenDialog"
                    />
                </f:content>
            </f:SimpleForm>

            <core:Fragment
                fragmentName="com.sumo.supplieronboarding.view.fragment.uploadfile"
                id="fileListFragment5"
                type="XML"
            />

            <VBox>
                <!-- List to display uploaded files -->
                <Dialog
                    id="fileUploader_box"
                    title="Upload Attachment Files"
                    class="sapUiResponsivePadding"
                >
                    <content>
                        <List
                            id="fileList2"
                            items="{/files}"
                            headerText=""
                        >
                            <CustomListItem id="ListItemID">
                                <HBox
                                    id="HBox1"
                                    justifyContent="SpaceBetween"
                                    alignItems="Center"
                                    width="100%"
                                >
                                    <!-- File name and download link -->
                                    <Link
                                        id="Link1"
                                        text="{fileName}"
                                        press="onDownload"
                                    >
                                        <customData>
                                            <core:CustomData
                                                key="fileUrl"
                                                value="{fileUrl}"
                                            />
                                        </customData>
                                    </Link>

                                    <!-- Delete button -->
                                    <Button
                                        id="DELButtonID"
                                        icon="sap-icon://delete"
                                        type="Transparent"
                                        press="onDeleteFile"
                                    />
                                </HBox>
                            </CustomListItem>
                        </List>
                    </content>
                </Dialog>
            </VBox>
        </VBox>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>

            <uxap:ObjectPageSection
                titleUppercase="false"
                id="CIRDsection"
                title="Company Identification and Regulatory Details"
