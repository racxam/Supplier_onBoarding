            <Label text="GST" /> 

<Select
    enabled="{/Enabled}"
    editable="{/Editable}"
    forceSelection="false"
    selectedKey="{/selectedGstFile}"
    id="gstDropdowns"
    items="{
        path: '/documentFiles/gst',
        templateShareable: false
    }"
>
    <core:Item
        key="{attachmentId}" 
        text="{fileName}"     
    />
</Select>

        </VBox>

    
      
    </Page>
</mvc:View>