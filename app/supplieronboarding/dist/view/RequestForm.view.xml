<mvc:View
    controllerName="com.sumo.supplieronboarding.controller.RequestForm"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
    xmlns:upload="sap.m.upload"
>
    <Page
        id="page1"
        title="{i18n>title}"
    >
        <VBox
            id="VBox1"
            height="100%"
            width="100%"
        >
            <f:SimpleForm
                id="SimpleForm"
                maxContainerCols="2"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanL="3"
                labelSpanM="3"
                columnsL="2"
                columnsM="2"
                emptySpanL="4"
                emptySpanM="4"
                adjustLabelSpan="false"
                class="editableForm"
                title="Request Form"
            >
                <f:content>
                    <Label
                        id="ValidityLabel"
                        text="Validity of digression vendor code upto"
                        required="true"
                    />
                    <DatePicker
                        id="datePicker"
                        valueFormat="yyyy-MM-dd"
                        displayFormat="long"
                        change="handleDateChange"
                    />

                    <Label
                        id="relatedpartyLabelID"
                        text="Is it a related party vendor code?"
                        required="true"
                    />
                    <RadioButtonGroup
                        id="radioGroup"
                        columns="2"
                        selectedIndex="0"
                    >
                        <buttons>
                            <RadioButton
                                id="RadioButton"
                                text="Yes"
                            />
                            <RadioButton
                                id="RadioButton_new"
                                text="No"
                            />
                        </buttons>
                    </RadioButtonGroup>

                    <Label
                        id="SupplierSpendTypeLabelID"
                        text="Supplier Spend Type"
                        required="true"
                    />
                    <Select
                        id="supplierSpendType"
                        selectedKey=""
                        items="{/results}"
                        change="handleComboBoxChange"
                    >
                        <items>
                            <core:Item
                                id="Item"
                                text="{SpendsTypes}"
                                key="{SpendsTypes}"
                            />
                        </items>
                    </Select>

                    <Label
                        id="NatureofActivityLabelID"
                        text="Nature of Activity"
                        required="true"
                    />
                    <Select
                        id="NatureofActivity"
                        selectedKey=""
                        items="{/results}"
                        change="handleComboBoxChange"
                    >
                        <items>
                            <core:Item
                                id="Item1"
                                text="{Activity}"
                                key="{Activity}"
                            />
                        </items>
                    </Select>

                    <Label
                        id="SectorLabelID"
                        text="Sector"
                        required="true"
                    />
                    <MultiComboBox
                        id="sectorComboBox"
                        placeholder="Select sectors"
                        items="{/results}"
                        selectionChange="handleMultiComboBoxChange"
                    >
                        <items>
                            <core:Item
                                id="Item100"
                                text="{Sectors} - {Description}"
                                key="{Sectors} - {Description}"
                            />
                        </items>
                    </MultiComboBox>

                    <Label
                        id="FunctionLabel"
                        text="Function &amp; Subfunction"
                        required="true"
                    />

                    <Select
                        id="FunctionandSubfunctionComboBox"
                        change="onDepartmentChange"
                        items="{
        path: 'departmentsModel>/results'
    }"
                    >
                        <core:Item
                            id="Item3"
                            key="{departmentsModel>DepartmentName}"
                            text="{departmentsModel>DepartmentName}"
                        />
                    </Select>

                    <MultiComboBox
                        id="childMultiComboBox"
                        visible="true"
                        placeholder="Select Subfunction"
                        items="{functionsModel>/Functions}"
                        selectionChange="handleMultiComboBoxChange"
                    >
                        <core:Item
                            id="Item4"
                            key="{functionsModel>}"
                            text="{functionsModel>}"
                        />
                    </MultiComboBox>

                    <Label
                        id="PANLabel"
                        text="PAN Card No."
                        required="true"
                    />
                    <Input
                        id="panInput"
                        placeholder="ABCDE1234F"
                        value="{/panCardValue}"
                        liveChange="onPanCardChange"
                        maxLength="10"
                    />
                    <Button
                        id="Button110"
                        text="Verify"
                        press="onSubmitpan"
                    />

                    <Label
                        id="PANCardAttachmentLabel"
                        text="PAN Card Attachment"
                        required="true"
                    />
                    <unified:FileUploader
                        id="fileUploaderPan"
                        fileType="jpg,png,pdf,docx,jpeg"
                        change="onFileUpload"
                        width="100%"
                    />
                    <Button
                        id="panuploadbutton"
                        text="{
            parts: [
                { path: '/documentFiles/pan' },
                { value: 'pan' }
            ],
            formatter: '.formatAttachmentButtonText'
        }"
                        press="onOpenDialog('pan')"
                    />

                    <Label
                        id="GSTINLabel"
                        text=" GSTIN No."
                        required="true"
                    />
                    <Input
                        id="gstinInput"
                        placeholder="22ABCDE1234F1Z5"
                        value="{/gstinValue}"
                        liveChange="onGSTINChange"
                        maxLength="15"
                    />
                    <Button
                        id="Button"
                        text="Verify"
                        press="onSubmitgst"
                    />

                    <Label
                        id="GSTAttachmentLabel"
                        text="GST Registration Certificate Attachment"
                        required="true"
                    />
                    <Label
                        text="GST Registration Certificate Attachment"
                        required="true"
                    />
                    <unified:FileUploader
                        id="fileUploaderGst"
                        fileType="jpg,png,pdf,docx,jpeg"
                        change="onFileUpload"
                        width="100%"
                    />
                    <Button
                        id="gstuploadbutton"
                        text="{
            parts: [
                { path: '/documentFiles/gst' },
                { value: 'gst' }
            ],
            formatter: '.formatAttachmentButtonText'
        }"
                        press="onOpenDialog('gst')"
                    />

                    <Label
                        id="SupplierNameLabel"
                        text="Supplier Full Legal Name"
                        required="true"
                    />
                    <Input
                        id="SupplierNameInput"
                        type="Text"
                        value="{/SupplierNameValue}"
                        liveChange="handleLiveChange"
                        maxLength="100"
                    />

                    <Label
                        id="SuppliertradeNameLabel"
                        text="Supplier Trade Name (GST)"
                        required="true"
                    />
                    <Input
                        id="SuppliertradeNameInput"
                        type="Text"
                        value="{/SuppliertradeNameValue}"
                        liveChange="handleLiveChange"
                        maxLength="150"
                    />

                    <Label
                        id="SupplierAddressLabel"
                        text="Supplier Address"
                        required="true"
                    />
                    <Input
                        id="SupplierAddressInput"
                        type="Text"
                        value="{/SupplierAddressValue}"
                        liveChange="handleLiveChange"
                        maxLength="150"
                    />

                    <Label
                        id="SupplierAddressgstLabel"
                        text="Supplier Address (GST)"
                        required="true"
                    />
                    <Input
                        id="SupplierAddressgstInput"
                        type="Text"
                        value="{/SupplierAddressgstValue}"
                        liveChange="handleLiveChange"
                        maxLength="150"
                    />

                    <Label
                        id="PrimaryFirstnameLabel"
                        text=" Primary Contact First Name"
                        required="true"
                    />
                    <Input
                        id="PrimaryFirstnameInput"
                        type="Text"
                        value="{/PrimaryFirstnameValue}"
                        liveChange="handleLiveChange"
                        maxLength="50"
                    />

                    <Label
                        id="PrimaryLastnameLabel"
                        text="Primary Contact Last Name"
                        required="true"
                    />
                    <Input
                        id="PrimaryLastnameInput"
                        type="Text"
                        value="{/PrimaryLastnameValue}"
                        liveChange="handleLiveChange"
                        maxLength="50"
                    />

                    <Label
                        id="EmailLabel"
                        text="Primary Contact Email"
                        required="true"
                    />
                    <Input
                        id="emailInput"
                        placeholder="Enter your email"
                        value="{/emailValue}"
                        liveChange="onEmailChange"
                    />
                    <Button
                        id="EmailButton"
                        text="Verify"
                        press="onSubmitemail"
                    />

                    <Label
                        id="MobilenoLabel"
                        text="Primary Contact Mobile Number"
                        required="true"
                    />
                    <Input
                        id="numberInput"
                        placeholder="Enter your Mobile number"
                        type="Number"
                        value="{/numberValue}"
                        liveChange="onNumberChange"
                    />
                    <Button
                        id="MobilenoButton"
                        text="Verify"
                        press="onSubmitnumber"
                    />
                </f:content>
            </f:SimpleForm>

            <core:Fragment
                fragmentName="com.sumo.supplieronboarding.view.fragment.uploadfile"
                id="fileListFragment1"
                type="XML"
            />

            <Toolbar id="Toolbar1">
                <ToolbarSpacer id="ToolbarSpacer" />
                <Button
                    id="SaveButton"
                    text="Save"
                    press="onFormSave"
                    width="100px"
                    type="Emphasized"
                />
                <Button
                    id="SubmitButton"
                    text="Submit"
                    press="onFormsubmit"
                    width="100px"
                    type="Emphasized"
                />

                <ToolbarSpacer id="ToolbarSpacer1" />
            </Toolbar>
        </VBox>
    </Page>
</mvc:View>
